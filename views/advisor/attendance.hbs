 <section id="sign_in-section" style="padding-top: 13em;">
        <div class="container">
            <div>
                <h1 class="text-left">STUDENT ATTENDANCE</h1>
            </div>
            <hr>
            <div class="row m-5">
                <form>
                    <div class="row">
                        <div class="col">
                            <label><i>COURSE :</i></label>
                        </div>
                        <div class="col">
                            <input type="text" style="border-width: 5px solid; border-radius: 6px;">
                        </div>
                        <div class="row">
                            <div class="col">
                                <label><i>SEARCH :</i></label>
                            </div>
                            <div class="col">
                                <input type="text" style="border-width: 5px solid; border-radius: 6px;" placeholder="Students">
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="row">
                <div class="col-md-9 col-12">
                    <div class="table">
                        <table class="table table-bordered">
                            <thead class="thead-dark">
                                <tr>
                                    <th scope="col">REG.NO</th>
                                    <th scope="col">NAME</th>
                                    <th scope="col">DEPT</th>
                                    <th scope="col">SEM</th>
                                    <th scope="col">COURSE</th>
                                    <th scope="col">ATTENDANCE</th>

                                </tr>
                            </thead>
                            <tbody>
                                     {{#each allStudents.details}}
                                 <tr>
                                <th scope="row">{{this.student_id}}</th>
                                <td>{{this.student_name}}</td>
                                <td>{{this.department_id}}</td>
                                <td>{{this.semester_id}}</td>
                                <td>{{this.course_name}}</td>
                                <td>{{this.attendance_percentage}}</td>
                              </tr>
                              {{/each}}
                               
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </section>